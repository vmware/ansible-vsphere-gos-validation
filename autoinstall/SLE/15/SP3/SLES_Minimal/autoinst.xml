      <listentry t="map">
        <media_url>cd:/?devices=/dev/sr0</media_url>
        <product>sle-module-basesystem</product>
        <product_dir>/Module-Basesystem</product_dir>
      </listentry>
      <listentry t="map">
        <media_url>cd:/?devices=/dev/sr0</media_url>
